node('master'){
    def jsonitem
    stage('AWS CLI Version'){
        sh "aws --version"
    }
    stage('create an ami'){
        def cmd = aws ec2 create-image --instance-id i-0c678277c4f5a17e2 --name "Myami" --description "amiserver"
        def output = sh(script: cmd)
        jsonitem = readJSON text: output
        println(jsonitem)
    }
	}
